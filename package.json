{
  "name": "lint-bot",
  "private": true,
  "engines": {"node": "^6.0.0"},
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/@cody-greene/eslint-config/strict.yml",
    "env": {"node": true, "es6": true}
  },
  "scripts": {
    "repl": "set -a;. .env && node",
    "start": "node lib/server.js",
    "test": "mocha -bR tap",
    "test-watch": "set -a;. .env && mocha -bwR min",
    "web": "set -a;. .env && nodemon -w lib lib/server.js"
  },
  "dependencies": {
    "@cody-greene/request": "0.1.0",
    "async": "2.0.1",
    "connect": "3.4.1",
    "eslint": "3.2.2",
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@cody-greene/eslint-config": "2.13.0",
    "mocha": "~2.4.5"
  }
}
